<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gzl.mapper.AdminMapper">
    <resultMap id="adminmap" type="com.gzl.domain.Admin">
        <id column="id" property="id"/>
        <result column="username" property="username"/>
        <result column="password" property="password"/>
        <result column="permission" property="permission"/>
    </resultMap>

    <resultMap id="planemap" type="com.gzl.domain.Plane">
        <id property="plane_id" column="plane_id" />
        <result property="cname" column="cname" />
        <result property="company" column="company" />
        <result property="ename" column="ename" />
        <result property="ptype" column="ptype" />
        <result property="img" column="image" />
        <result property="first" column="first" />
        <result property="license" column="license" />
        <result property="wings" column="wings" />
        <result property="usage" column="toUse" />
        <result property="seats" column="seats" />
        <result property="flight" column="flight" />
        <result property="price" column="price" />
        <result property="power" column="power" />
    </resultMap>

    <resultMap id="typemap" type="com.gzl.domain.Type">
        <id property="type_id" column="type_id" />
        <result property="type_name" column="type_name" />
        <result property="type_img" column="type_img" />
        <result property="introduction" column="introduction" />
    </resultMap>



</mapper>